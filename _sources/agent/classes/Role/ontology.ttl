@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://www.commoncoreontologies.org/Role> a owl:Ontology ;
    rdfs:label "CCO Role"@en ;
    rdfs:comment "Ontology for the Role class from Common Core Ontologies"@en ;
    dc:creator "CUBRC, INC" ;
    dcterms:license <https://opensource.org/licenses/BSD-3-Clause> ;
    owl:versionInfo "Version 2.0" .

# Class Definitions

cco:Role a owl:Class ;
    rdfs:label "Role"@en ;
    rdfs:comment "A role is a realizable entity that exists because of the realization of some characteristic or set of characteristics"@en .

cco:OccupationalRole a owl:Class ;
    rdfs:label "Occupational Role"@en ;
    rdfs:comment "A role that inheres in an agent by virtue of their occupation"@en ;
    rdfs:subClassOf cco:Role .

cco:SocialRole a owl:Class ;
    rdfs:label "Social Role"@en ;
    rdfs:comment "A role that inheres in an agent by virtue of social relationships"@en ;
    rdfs:subClassOf cco:Role .
