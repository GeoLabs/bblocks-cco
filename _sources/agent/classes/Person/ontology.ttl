@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://www.commoncoreontologies.org/Person> a owl:Ontology ;
    rdfs:label "CCO Person"@en ;
    rdfs:comment "Ontology for the Person class from Common Core Ontologies"@en ;
    dc:creator "CUBRC, INC" ;
    dcterms:license <https://opensource.org/licenses/BSD-3-Clause> ;
    owl:versionInfo "Version 2.0" .

# Class Definition

cco:Person a owl:Class ;
    rdfs:label "Person"@en ;
    rdfs:comment "A person is an agent that is a human being"@en ;
    rdfs:subClassOf cco:Agent .

# Properties

cco:hasBirthDate a owl:DatatypeProperty ;
    rdfs:label "has birth date"@en ;
    rdfs:comment "Relates a person to their date of birth"@en ;
    rdfs:domain cco:Person ;
    rdfs:range xsd:date .

cco:hasRole a owl:ObjectProperty ;
    rdfs:label "has role"@en ;
    rdfs:comment "Relates an agent to a role they hold"@en ;
    rdfs:domain cco:Agent ;
    rdfs:range cco:Role .

cco:hasAffiliation a owl:ObjectProperty ;
    rdfs:label "has affiliation"@en ;
    rdfs:comment "Relates a person to an organization they are affiliated with"@en ;
    rdfs:domain cco:Person ;
    rdfs:range cco:Organization ;
    owl:inverseOf cco:hasAffiliate .
