@prefix cco: <https://www.commoncoreontologies.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .

<https://www.commoncoreontologies.org/Organization> a owl:Ontology ;
    rdfs:label "CCO Organization"@en ;
    rdfs:comment "Ontology for the Organization class from Common Core Ontologies"@en ;
    dc:creator "CUBRC, INC" ;
    dcterms:license <https://opensource.org/licenses/BSD-3-Clause> ;
    owl:versionInfo "Version 2.0" .

# Class Definition

cco:Organization a owl:Class ;
    rdfs:label "Organization"@en ;
    rdfs:comment "An organization is a collective of persons organized for a common purpose"@en ;
    rdfs:subClassOf cco:Agent .

# Properties

cco:foundedDate a owl:DatatypeProperty ;
    rdfs:label "founded date"@en ;
    rdfs:comment "Relates an organization to the date it was founded"@en ;
    rdfs:domain cco:Organization ;
    rdfs:range xsd:date .

cco:hasAffiliate a owl:ObjectProperty ;
    rdfs:label "has affiliate"@en ;
    rdfs:comment "Relates an organization to persons affiliated with it"@en ;
    rdfs:domain cco:Organization ;
    rdfs:range cco:Person ;
    owl:inverseOf cco:hasAffiliation .

cco:hasSubOrganization a owl:ObjectProperty ;
    rdfs:label "has sub-organization"@en ;
    rdfs:comment "Relates an organization to its sub-organizations"@en ;
    rdfs:domain cco:Organization ;
    rdfs:range cco:Organization .
